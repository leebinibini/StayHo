<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.LocationInfoMapper">
    <select id="selectSido" resultType="String">
        SELECT sido FROM location_info
        WHERE LENGTH(sido)>2
        GROUP BY sido
    </select>
    <select id="selectSigungu" resultType="String" parameterType="String">
        SELECT sigungu FROM location_info
        WHERE sido=#{sido}
    </select>
</mapper>